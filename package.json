{
  "name": "vistoso",
  "version": "0.0.7",
  "packageManager": "pnpm@7.9.5",
  "description": "Beautiful Colors Lookup in CLI",
  "author": "Joruno-w",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Joruno-w",
  "homepage": "https://github.com/Joruno-w/vistoso#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joruno-w/vistoso.git"
  },
  "bin": {
    "vistoso": "./bin/vistoso.js"
  },
  "bugs": "https://github.com/Joruno-w/vistoso/issues",
  "keywords": [
    "color",
    "colors",
    "colour",
    "colours",
    "lookup",
    "vistoso"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "bin",
    "color.json"
  ],
  "scripts": {
    "dev": "esno src/cli.ts",
    "build": "esno scripts/build.ts && tsup src/cli.ts src/index.ts --format cjs,esm --dts --minify",
    "prepare": "npm run build",
    "release": "npx bumpp --commit --tag --push && npm publish",
    "web": "npm -C web run dev"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@types/chroma-js": "^2.4.0",
    "@types/color-namer": "^1.3.0",
    "@types/node": "^18.15.11",
    "bumpp": "^9.1.0",
    "chroma-js": "^2.4.2",
    "color-namer": "^1.4.0",
    "eslint": "^8.37.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.2.0",
    "pnpm": "^8.1.1",
    "rimraf": "^4.4.1",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.3",
    "unbuild": "^1.2.0",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  },
  "dependencies": {
    "chalk": "4.1.0"
  }
}
